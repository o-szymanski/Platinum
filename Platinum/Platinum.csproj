<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Sorting Library</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Company>o-szymanski</Company>
    <Authors>o-szymanski</Authors>
    <Copyright>2022</Copyright>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <RepositoryUrl>https://github.com/o-szymanski/Platinum</RepositoryUrl>
    <PackageTags>Sort, Sorter, Sorting</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

</Project>
